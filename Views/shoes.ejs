        <div class="container">
            <%if(brand){%>
              <h1><%=brand%></h1> 
            <%} else {%>
            <h1>Shoes Collections</h1>
            <%}%>
            <ul>
                <li><a href='/shoes/<%=brand%>/Running'> Running</a></li>
                <li><a href='/shoes/<%=brand%>/Basketball'>Basketball</a></li>
                <li><a href='/shoes/<%=brand%>/Soccer'>Soccer</a></li>
                <li><a href='/shoes/<%=brand%>/Casual'>Casual</a></li>
            </ul>
            <table>
                <tr>
                    <th>name</th>
                    <th>brand</th>
                    <th>type</th>
                    <th>price</th>
                </tr>
                
                    <%for( shoes of data ){%>
                        <tr>
                        <td><%= shoes.dataValues.name%></td>
                        <td><%= shoes.dataValues.brand%></td>
                        <td><%= shoes.dataValues.type%></td>
                        <td><%= shoes.dataValues.price%></td>
                        <td><a href="/shoes/cart/<%=shoes.dataValues.id%>">Add To Cart</a></td>                        
                        <td><a href="/shoes/edit/<%=shoes.dataValues.id%>">| edit</a></td>
                    </tr>
                    <%}%>
                
            </table>
